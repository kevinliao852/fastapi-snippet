{
  "FastAPI Router": {
    "prefix": "frt",
    "body": ["from fastapi import APIRouter", "router = APIRouter()"]
  },
  "FastAPI Get Route": {
    "prefix": "frg",
    "body": [
      "from fastapi import APIRouter",
      "router = APIRouter()",
      "",
      "@router.get(\"/\", tags=[\"${1:tag}\"])",
      "async def ${2:route_name}():",
      "\treturn {\"message\": \"${3:Hello World}\"}"
    ]
  },
